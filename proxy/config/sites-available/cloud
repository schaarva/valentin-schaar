server {
	listen 443 ssl;

	ssl_certificate /etc/letsencrypt/live/valentin-schaar.de/fullchain.pem;
	ssl_certificate_key /etc/letsencrypt/live/valentin-schaar.de/privkey.pem;

	server_name cloud.valentin-schaar.de;

	location / {
		proxy_pass http://cloud:80;

		proxy_set_header Host $host;
		proxy_set_header X-Real-IP $remote_addr;
		proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
	}
}

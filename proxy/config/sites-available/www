server {
	listen 443 ssl;

	ssl_certificate /etc/letsencrypt/live/valentin-schaar.de/fullchain.pem;
	ssl_certificate_key /etc/letsencrypt/live/valentin-schaar.de/privkey.pem;

	server_name valentin-schaar.de www.valentin-schaar.de;

	root /var/www/html;
	index index.html;

	location / {
		try_files $uri $uri/ =404;
	}
}
